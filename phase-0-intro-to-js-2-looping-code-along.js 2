for (var i = 0; i < 10; i++) {
    console.log(i);
  }
  
  var i = 0;
  while (i < 10) {
    console.log(i);
    i++;
  }
  var i = 0;
  do {
    console.log(i);
    i++;
  } while (i < 10);
  var numbers = [1, 2, 3, 4, 5];
  for (var number of numbers) {
    console.log(number);
  }
  
  var numbers = [1, 2, 3, 4, 5];
  for (var number of numbers) {
    console.log(number);
    if (number === 3) {
      break;
      console.log("break");
      break;
      console.log("break");
      break;
      console.log("break");
    }
    if (number === 5) {
      continue;
      console.log("continue");
      continue;
      console.log("continue");   
  }
  
    if (number === 2) {
      break;
      console.log("break");
      break;
  
      console.log("break");
      break;
  }